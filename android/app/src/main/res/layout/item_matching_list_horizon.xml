<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <View
        android:layout_width="138dp"
        android:layout_height="91dp"
        app:cardCornerRadius="15dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:background="@drawable/item_matching_list_horizon_background"
        android:paddingRight="7dp"/>

    <TextView
        android:id="@+id/tv_item_matching_list_horizon_nickname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="11dp"
        android:layout_marginTop="11dp"
        android:textSize="12sp"
        android:fontFamily="@font/pretendardregular"
        android:textFontWeight="700"
        android:text="닉네임"
        android:textColor="#366943"
        />

    <TextView
        android:layout_width="40dp"
        android:layout_height="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="33.5dp"
        android:text="학과"
        android:textColor="#3B3B3B"
        android:textFontWeight="400"
        android:textSize="10sp"
        android:textAlignment="center"/>

    <View

        android:id="@+id/v_item_matching_list_horizon_v2"
        android:layout_width="80dp"
        android:layout_height="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="21dp"
        android:background="@drawable/shape_100radius_eeeeee"/>


    <TextView
        android:id="@+id/tv_item_matching_list_horizon_department"
        android:layout_width="56dp"
        android:layout_height="16dp"
        app:layout_constraintTop_toTopOf="@id/v_item_matching_list_horizon_v2"
        app:layout_constraintBottom_toBottomOf="@id/v_item_matching_list_horizon_v2"
        app:layout_constraintStart_toStartOf="@id/v_item_matching_list_horizon_v2"
        app:layout_constraintEnd_toEndOf="@id/v_item_matching_list_horizon_v2"
        android:text="OOOO 학과"
        android:textSize="10sp"
        android:textFontWeight="400"
        android:textAlignment="center"
        android:textColor="#3B3B3B"/>
    
    <TextView
        android:layout_width="40dp"
        android:layout_height="15dp"
        android:text="정보"
        android:textSize="10sp"
        android:textFontWeight="400"
        android:textColor="#3B3B3B"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="63.5dp"
        android:textAlignment="center"/>

    <View
        android:id="@+id/v_item_matching_list_horizon_v3"
        android:layout_width="53dp"
        android:layout_height="20dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="61dp"
        android:layout_marginStart="37dp"
        android:background="@drawable/shape_100radius_eeeeee"/>
    
    <TextView
        android:id="@+id/tv_item_matching_list_horizon_country"
        android:layout_width="19dp"
        android:layout_height="15dp"
        android:textAlignment="center"
        android:text="국가"
        android:textFontWeight="400"
        android:textSize="10sp"
        android:textColor="#3B3B3B"
        app:layout_constraintTop_toTopOf="@id/v_item_matching_list_horizon_v3"
        app:layout_constraintBottom_toBottomOf="@id/v_item_matching_list_horizon_v3"
        app:layout_constraintStart_toStartOf="@id/v_item_matching_list_horizon_v3"
        app:layout_constraintEnd_toEndOf="@id/v_item_matching_list_horizon_v3"/>


    <View
        android:id="@+id/v_item_matching_list_horizon_v4"
        android:layout_width="34dp"
        android:layout_height="20dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/v_item_matching_list_horizon_v3"
        android:layout_marginTop="61dp"
        android:layout_marginStart="4dp"
        android:background="@drawable/shape_100radius_eeeeee"/>

    <TextView
        android:id="@+id/tv_item_matching_list_horizon_gender"
        android:layout_width="10dp"
        android:layout_height="15dp"
        android:textAlignment="center"
        android:text="성별"
        android:textFontWeight="400"
        android:textSize="10sp"
        android:textColor="#3B3B3B"
        app:layout_constraintTop_toTopOf="@id/v_item_matching_list_horizon_v4"
        app:layout_constraintBottom_toBottomOf="@id/v_item_matching_list_horizon_v4"
        app:layout_constraintStart_toStartOf="@id/v_item_matching_list_horizon_v4"
        app:layout_constraintEnd_toEndOf="@id/v_item_matching_list_horizon_v4"/>






</androidx.constraintlayout.widget.ConstraintLayout>